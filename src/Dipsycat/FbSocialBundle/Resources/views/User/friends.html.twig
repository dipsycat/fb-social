{% extends 'DipsycatFbSocialBundle::layout.html.twig' %}
{% block title %}
    Friends
{% endblock %}
{% block wrapper %}
    {{ parent() }}
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Friends</h1>
        </div>
    </div>
    <div class="row">
        <ul>
            {% for friend in friends %}
                <li>{{ friend.username }}</li>
                {% endfor %}
        </ul>
    </div>

{% endblock %}

